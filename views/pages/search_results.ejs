<html>
    <head>
    <meta charset="utf-8">

    <link rel="icon" type="image/vnd.microsoft.icon" href="/images/paw.png">

    <link href="https://fonts.googleapis.com/css?family=Dosis|Josefin+Sans" rel="stylesheet"> 

    <title>Ausgabe</title>

</head>
    <body>
        <h1>Diese Hunde wurden gefunden:</h1>

        <% if(message) { %>
            <%= message %>
        <% } %>

        <% dogs.forEach(function (dog) { %>
            <li>
                <%= dog.name %>, <%= dog.height %>, <%= dog.bread %>, <%= dog.gender %>, <%= dog.age %>, <%= dog.castrated %>
                <img src="<%= dog.img %>" style="height: 150px; width: 150px; height:150px;"/>
            </li>                        
        <% }) %>
    </body>
</html>