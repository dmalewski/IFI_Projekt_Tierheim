<% dogs.forEach(function (dog) { %>
<div class="elementgrid">
 
  <a href="details" >
    <p class="center figcap"><%= dog.name %> </p>
    <hr />
    <figure class="image-wrapper">
      <img src="<%= dog.img %>"></img>
    </figure> 
    <figcaption>
        <P class="center figbread"><%= dog.bread %></P>
    </figcaption>
  </a>
  <div id="attribute">

    <% if (dog.height.match(/Klein/ig)) { %>
      <img id="icon" src="/images/small_s.png" title="Klein"/>
    <% } %>

    <% if (dog.height.match(/Mittel/ig)) { %>
      <img id="icon" src="/images/middle_m.png" title="Mittelgroß "/>
    <% } %>

    <% if (dog.height.match(/Groß/ig)) { %>
      <img id="icon" src="/images/big_l.png" title="Groß"/>
    <% } %>

    <% if (dog.age.match(/jung/ig)) { %>
      <img id="icon" src="/images/baby.png" title="jung"/>
    <% } %>

     <% if (dog.age.match(/erwachsen/ig)) { %>
      <img id="icon" src="/images/adult.png" title="erwachsen"/>
    <% } %>

    <% if (dog.age.match(/senior/ig)) { %>
      <img id="icon" src="/images/senior.png" title="senior"/>
    <% } %>

    <% if (dog.gender.match(/männlich/ig)) { %>
      <img id="icon" src="/images/male_legend.png" title="männlich"/>
    <% } %>

    <% if (dog.gender.match(/weiblich/ig)) { %>
      <img id="icon" src="/images/female_legend.png" title="weiblich"/>
    <% } %>

    <% if (dog.castrated.match(/Ja/ig)) { %>
        <img id="icon" src="/images/castrated_unfilled.png" title="Kastriert"/>
    <% } %>

  
    <% const trait = dog.traits %>
    <%= trait %>
  
  
    <!--<img id="icon" src="/images/likes_cat.png" title="traits"/>-->
   
 
 </div>
 
</div>
<% }) %>